{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },

  "JwtAuth": {
    "Issuer": "RayPI",
    "Audience": "MyAudience",
    "SecurityKey": "This is JWT Secret Key",
    "WebExp": 12,
    "AppExp": 1440,
    "MiniProgramExp": 1440,
    "OtherExp": 1
  },

  "Db": {
    "DbType": "SqlServer",

    "SqlServer": {
      "ConnStr": "Server=(localdb)\\mssqllocaldb;Database=RayPI;Trusted_Connection=True;"
    },
    "Sqlite": {
      "ConnStr": "Data Source=.\\RayPI.db"
    }
  },
  "EventBus": {
    "SubscriptionClientName": "RayPI",//消费者端名称（将作为队列名称）
    "EventBusRetryCount": 3,//连接重试次数
    "EventBusConnection": "localhost",//连接地址
    "EventBusUserName": "",//为空则使用默认用户
    "EventBusPassword": ""//为空则使用默认密码
  }
}
